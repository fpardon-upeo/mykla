    <validationRules>
        <fullName>Disqualify_Reason_Mandatory</fullName>
        <active>true</active>
        <description>to make sure a disqualify reason is added when a lead is disqualified</description>
        <errorConditionFormula>AND(
TEXT( Status ) = &quot;Disqualified&quot;,
ISBLANK( Disqualified_Reason__c )
)</errorConditionFormula>
        <errorDisplayField>Disqualified_Reason__c</errorDisplayField>
        <errorMessage>Please add a reason this lead is disqualified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inactive_Pricebook</fullName>
        <active>true</active>
        <errorConditionFormula>Price_Book__r.IsActive = FALSE</errorConditionFormula>
        <errorMessage>The selected pricebook is inactive, please select an active one</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Info_complete_before_conversion</fullName>
        <active>true</active>
        <errorConditionFormula>IsConverted
&amp;&amp;
 Ready_for_Conversion__c =false</errorConditionFormula>
        <errorMessage>Please fill in all data with the button &apos;Prepare for conversion&apos; before converting</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Label_needed_before_conversion</fullName>
        <active>true</active>
        <errorConditionFormula>IsConverted = TRUE
&amp;&amp;
 ISBLANK(Labels__c )</errorConditionFormula>
        <errorMessage>Please add at least one label before converting</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VAT_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISBLANK(VAT_Number__c )),
NOT( REGEX( VAT_Number__c , &quot;[BE]{2}[0-9]{10}&quot;)),
NOT( REGEX( VAT_Number__c , &quot;[NL]{2}[0-9]{10}&quot;)),
$Profile.Name &lt;&gt; &apos;Salesforce API Only System Integrations&apos;)</errorConditionFormula>
        <errorMessage>The VAT format needs to be &quot;BE0477472701&quot;</errorMessage>
    </validationRules>
</CustomObject>
