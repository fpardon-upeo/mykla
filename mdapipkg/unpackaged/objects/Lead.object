    <fields>
        <fullName>IAK_Pre_Scoring__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Prime_Location_Medium_High_Foot_Traffic__c , 1, 0) +
IF( Finances_Healthy_Business__c  , 1, 0) +
IF( Concept_Alignment_with_Brand__c  , 1, 0)+
IF( Concept_Knowledge_of_Ingredients__c  , 1, 0)+
IF( Concept_Opportunity_for_All_Lines__c  , 1, 0)+
IF( Concept_Sustainability__c , 1, 0)+
IF( Mkt_Sales_Existing_Website_Webshop__c , 1, 0)+
IF( Mkt_Sales_Opport_Workshops_events__c  , 1, 0)+
IF( Mkt_Sales_Social_Media_Platforms__c  , 1, 0)+
IF( MKT_Sales_Staff_Trained_in_Makeup__c  , 1, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>IAK Pre Scoring</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
