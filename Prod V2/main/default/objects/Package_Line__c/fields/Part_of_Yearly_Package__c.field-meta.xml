<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Part_of_Yearly_Package__c</fullName>
    <description>Formula that checks if the Line is part of an active Yearly Package</description>
    <externalId>false</externalId>
    <formula>AND(
 Package__r.Year__c = TRUE,
 Package__r.Active__c = TRUE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Part of Yearly Package</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
